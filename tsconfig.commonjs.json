{
    "compilerOptions": {
      "target": "ES2022",
      "module": "CommonJS",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "resolveJsonModule": true,
      "lib": ["ES2022", "DOM", "DOM.Iterable"],
      "noEmit": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  